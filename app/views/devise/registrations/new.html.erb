
<div id=registrationbk class="row">

<div class="col-sm-5 logo-wrapper">
      <div class="logo">
        <%=image_tag("brand/LogoWhite.png", class: "imgSize", alt: "Askify-Logo") %>
  </div>
</div>

<div class="col-sm-7">
    <div id="formBox" >
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name), class: "form") do |f| %>
        <%= devise_error_messages! %>
        <div id="formReg" >
            <div class="form-group" id="fName">
               <%= f.text_field :first_name, autofocus: true,
               placeholder:"First Name"%>
            </div>

            <div class="form-group" id="lName">
               <%= f.text_field :last_name, placeholder:"Last Name"%>
            </div>

            <div class="form-group" id="uMat">
                <%= f.text_field :student_id, autocomplete: "off" , placeholder:"Matricula"%>
            </div>

            <div class="form-group" id="uEmail">
                <%= f.email_field :email, autocomplete: "email" , placeholder:"Email"%>
            </div>

            <div class="form-group" id="uName">
               <%= f.text_field :user_name, placeholder:"Username"%>
            </div>


            <div class="form-group" id="uName">
                <%= f.date_field :birth_date %>
            </div>

            <div class="form-group" id="uPass">
            <%= f.password_field :password, autocomplete: "off", placeholder:"Password"%>
            </div>

            <div class="form-group" id="uPassConf">
            <%= f.password_field :password_confirmation, autocomplete: "off", placeholder:"Password Confirmation"%>
            </div>

            <div class="form-check" id="uGender">
              <%= check_box_tag(:gender) %>
              <%= label_tag(:gender, "Female") %>
              <%= check_box_tag(:gender) %>
              <%= label_tag(:gender, "Male") %>
            </div>

            <div class="actions" id="signUp">
            <%= f.submit "Sign up", class:"submit-form btn btn-primary " %>

            <%= render "devise/shared/links" %>
            <% end %>
            </div>
        </div>
    </div>
</div>



</div>
